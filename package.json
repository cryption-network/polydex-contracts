{
  "name": "polydex-contracts",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "compile": "npx hardhat compile",
    "coverage": "hardhat coverage",
    "lint": "solhint --max-warnings 0 \"contracts/**/*.sol\"",
    "prepublish": "rimraf build contracts/build artifacts cache",
    "prepare": "env COMPILE_MODE=production npm run compile",
    "deploy": "npx hardhat run scripts/deploymentScript.js --network matic",
    "test": "hardhat test",
    "deploy-migrator:main": "HARDHAT_NETWORK=matic npx hardhat run scripts/deploy/deploy_migrator.js",
    "deploy-migrator:test": "HARDHAT_NETWORK=mumbai npx hardhat run scripts/deploy/deploy_migrator.js",
    "deployFactory:test": "HARDHAT_NETWORK=mumbai npx hardhat run scripts/deploy/deployFactory.js",
    "deployRouter:test": "HARDHAT_NETWORK=mumbai npx hardhat run scripts/deploy/deployRouter.js",
    "deployFactory:main": "HARDHAT_NETWORK=matic npx hardhat run scripts/deploy/deployFactory.js",
    "deployRouter:main": "HARDHAT_NETWORK=matic npx hardhat run scripts/deploy/deployRouter.js",
    "deployFarm:test": "HARDHAT_NETWORK=mumbai npx hardhat run scripts/deploy/deploy_farm.js",
    "deployCNTStaker:test": "HARDHAT_NETWORK=mumbai npx hardhat run scripts/deploy/deploy_cnt_staker.js",
    "deployCNTStaker:main": "HARDHAT_NETWORK=matic npx hardhat run scripts/deploy/deploy_cnt_staker.js",
    "deployConverter:test": "HARDHAT_NETWORK=mumbai npx hardhat run scripts/deploy/deploy_converter.js",
    "deployConverter:main": "HARDHAT_NETWORK=matic npx hardhat run scripts/deploy/deploy_converter.js",
    "deployStakingPool:test": "HARDHAT_NETWORK=mumbai npx hardhat run scripts/deploy/deploy_staking_pool.js",
    "deployStakingPool:main": "HARDHAT_NETWORK=matic npx hardhat run scripts/deploy/deploy_staking_pool.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-etherscan": "^2.1.4",
    "@nomiclabs/hardhat-solhint": "^2.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/hardhat-upgrades": "^1.6.0",
    "chai": "^4.3.4",
    "ethereum-waffle": "^3.3.0",
    "ethers": "^5.1.3",
    "hardhat": "^2.2.1"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^3.4.0",
    "bignumber.js": "^9.0.1",
    "chalk": "^4.1.1",
    "dotenv": "^8.2.0",
    "solidity-coverage": "^0.7.16"
  }
}
